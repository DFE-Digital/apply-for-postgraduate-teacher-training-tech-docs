<% if operation.responses.any? %>
### Possible reponses

<% operation.responses.each do |key,response| %>
#### HTTP <code><%= key %></code> - <%= response.description %>

<% if response.content['application/json']
response_body = if response.content['application/json']['example']
  render_object_as_json_block(response.content['application/json']['example'])
else
  render_schema_example_as_json_block(response.content['application/json'].schema)
end
end %>

<% if response_body %>
<pre><code><%= response_body %></code></pre>
<% end %>

<% end %>
<% end %>
